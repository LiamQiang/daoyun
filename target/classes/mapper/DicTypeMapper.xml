<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lq.daoyun.mapper.DicTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lq.daoyun.pojo.DicType">
    <result column="id" property="id" />
        <result column="paraname" property="paraname" />
        <result column="code" property="code" />
        <result column="description" property="description" />
        <result column="identity" property="identity" />
        <result column="creator" property="creator" />
        <result column="creatordate" property="creatordate" />
        <result column="modificationdate" property="modificationdate" />
        <result column="modifier" property="modifier" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        paraname, code, description, identity, creator, creatordate, modificationdate, modifier
    </sql>

</mapper>
