<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lq.daoyun.mapper.MenuMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lq.daoyun.pojo.Menu">
    <result column="id" property="id" />
        <result column="parentmenu_number" property="parentmenuNumber" />
        <result column="menuname" property="menuname" />
        <result column="button_english_logo" property="buttonEnglishLogo" />
        <result column="button_chinese_logo" property="buttonChineseLogo" />
        <result column="is_menu" property="isMenu" />
        <result column="creator" property="creator" />
        <result column="creationdate" property="creationdate" />
        <result column="modificationdate" property="modificationdate" />
        <result column="modifier" property="modifier" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        parentmenu_number, menuname, button_english_logo, button_chinese_logo, is_menu, creator, creationdate, modificationdate, modifier
    </sql>

</mapper>
