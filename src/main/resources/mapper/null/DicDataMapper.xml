<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lq.daoyun.mapper.DicDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lq.daoyun.pojo.DicData">
    <result column="id" property="id" />
        <result column="dic_type" property="dicType" />
        <result column="dic_key" property="dicKey" />
        <result column="dic_value" property="dicValue" />
        <result column="defaultvalue" property="defaultvalue" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        dic_type, dic_key, dic_value, defaultvalue
    </sql>

</mapper>
